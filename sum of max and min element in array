import java.util.Scanner;
public class Main {
public static void main(String[] args){
 Scanner sc=new Scanner(System.in);
 int max = Integer.MIN_VALUE;
 int min = Integer.MAX_VALUE;
 int size=sc.nextInt();
 int arr[]=new int[size];
 int i;
 for (i=0;i<size;i++){
   arr[i]=sc.nextInt();
 }
 for (int a:arr){
            if (a>max){
              max = a;
            }
            if (a<min){
                min=a;
            }
        }
  System.out.println(min+max);
 }
}
