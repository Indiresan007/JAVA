import java.util.Scanner;
public class Main {
   public static void  main(String[] goodmorning){
      Scanner sc = new Scanner(System.in);
      int N = sc.nextInt();
      int i;
      int r=0;
      int sum=0;
      for(i=1;N>0;i++){
        r=N%10;
        if(r%2!=0){
          sum=sum+r;
        }
        N=N/10;
      }
      System.out.println(sum);
    }
}
