import java.util.Scanner;
class Main{
  public static void main(String[] args){
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int i;
    int t=0;
    for(i=0;i<n;i++){
      for(int j=t;j<n;j++){
        System.out.print("* ");
      }
      t=t+1;
      System.out.println();
    }
  }
}
