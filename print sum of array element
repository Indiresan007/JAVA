import java.util.Scanner;
public class Main {
public static void main(String[] args){
 Scanner sc=new Scanner(System.in);
 int sum=0;
 int size=sc.nextInt();
 int arr[]=new int[size];
 int i;
 for (i=0;i<size;i++){
   arr[i]=sc.nextInt();
 }
 for (i=0;i<size;i++){
  sum+=arr[i];
 }
 System.out.println(sum);
 }
}
