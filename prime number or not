import java.util.Scanner;
public class Main {
public static void main(String[] args){
 Scanner sc=new Scanner(System.in);
 int num1=sc.nextInt();
 //int num2=sc.nextInt();
 //int num2=sc.nextInt();
 int i;
 int c=0;
 for (i=1;i<=num1;i++){
   if (num1%i==0){
     c+=1;
   }
   
 }
 if (c==2){
   System.out.println("Prime number");
 }
 else{
   System.out.println("Not a Prime number");
 }
 }
}
