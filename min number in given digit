import java.util.Scanner;
public class Main {
   public static void  main(String[] goodmorning){
      Scanner sc = new Scanner(System.in);
      int N = sc.nextInt();
      int i;
      int min=N%10;
      for(i=1;N>0;i++){
        if(N%10<min){
          min=N%10;
        }
        N=N/10;
      }
      System.out.println("min number in given digit: "+min);
    }
}
