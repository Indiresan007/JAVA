import java.util.Scanner;
class Main{
  public static void main(String[] args){
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int i;
    int j;
    int l=0;
    for(i=0;i<n;i++){
      l=l+1;
      for(j=0;j<l;j++){
        System.out.print("* ");
      }
    System.out.println();
    }
  }
}
