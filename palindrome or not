import java.util.*;
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int n=sc.nextInt();
      int temp=n;
      int i=0,r=0;
      int sum=0;
      for(i=0;n>0;i++){
        r=n%10;
        sum=(sum*10)+r;
        n=n/10;
      }
      if(temp==sum){
      System.out.println("palindrome");
      }
      else{
        System.out.println("not a palindrome");
      }
  }
}
