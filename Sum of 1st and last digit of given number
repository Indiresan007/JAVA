import java.util.Scanner;
public class Main {
   public static void  main(String[] goodmorning){
      Scanner sc = new Scanner(System.in);
      int N = sc.nextInt();
      int i;
      int sum=0;
      for(i=1;N>0;i++){
        if(i==1){
        sum=sum+N%10;
        }
        if((N/10)==0){
        sum=sum+N%10;
        }
        N=N/10;
      }
      System.out.println(sum);
    }
}
